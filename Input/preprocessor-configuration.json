{

    "VarianceThreshold": {
        "module": "sklearn.feature_selection",
        "kwargs": {
        "threshold": 0
        }
    },

    "MissingValuesSelector": {
        "module": "transform",
        "kwargs": {
        "threshold": 0.3
        }
    },

    "PowerTransformer": {
        "module": "sklearn.preprocessing",
        "kwargs": {
        "standardize": false
        },
        "do_column_select": true
    },

    "SimpleImputer": {
        "module": "sklearn.impute",
        "kwargs": {
        "strategy": "median"
        }
    },
    
    "MedianScaler": {
        "module": "transform",
        "do_column_select": true
    }

}